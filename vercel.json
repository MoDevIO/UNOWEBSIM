{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "express",
  "functions": {
    "server/index.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "NODE_ENV": "production"
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/index.js"
    },
    {
      "source": "/ws",
      "destination": "/index.js"
    },
    {
      "source": "/examples/:path*",
      "destination": "/index.js"
    }
  ],
  "headers": [
    {
      "source": "/public/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
